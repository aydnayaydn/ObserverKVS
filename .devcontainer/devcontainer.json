{
    "name": "observer-kvs-devcontainer",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "settings": {
      "go.gopath": "/go",
      "go.goroot": "/usr/local/go",
      "go.toolsGopath": "/go/bin",
      "go.useLanguageServer": true,
      "go.inferGopath": true,
      "go.formatTool": "gofmt"
    },
    "extensions": ["golang.Go"]
  }